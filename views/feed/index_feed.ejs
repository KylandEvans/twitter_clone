<% layout('layout/boilerplate') -%>
<link rel="stylesheet" href="CSS/index_feed.css" />

<main>
	<section class="feed-outer">
		<header class="header-outer">
			<div class="header-head">
				<div class="profile-image-outer">
					<img src="Images/test_Image.jpg" alt="" class="profile-image" />
				</div>
				<div class="logo-image-outer">
					<img src="Icons/logo_blue.svg" alt="" class="logo-header" />
				</div>
				<div class="header-filler-blank"></div>
			</div>
			<div class="header-buttons">
				<button class="for-you-header-btn header-text-active">For you</button>
				<button class="following-header-btn">Following</button>
			</div>
		</header>
		<section class="feed">
			<% for(let i = 0; i < 50; i++) {%>
			<div class="feed-item-outer">
				<div class="feed-proile-image-wrapper">
					<div class="image-border-wrapper">
						<a href="#">
							<img src="Images/test_image.jpg" alt="" class="feed-profile-image" />
						</a>
					</div>
				</div>
				<div class="feed-item-inner-body">
					<div class="feed-item-header">
						<div class="feed-item-header-wrapper">
							<h4 class="feed-item-name feed-text">Kyland Evans</h4>
							<h4 class="text-muted">@Kyland_Evans</h4>
							<div class="seperator"></div>
							<h4 class="text-muted">2h</h4>
						</div>
						<div class="feed-item-options-button-wrapper">
							<!-- <img src="Icons/options.svg" alt="" class="options-img" /> -->
							<div class="options-button">
								<svg viewBox="0 0 24 24" class="options-img">
									<g>
										<path
											d="M3 12c0-1.1.9-2 2-2s2 .9 2 2-.9 2-2 2-2-.9-2-2zm9 2c1.1 0 2-.9 2-2s-.9-2-2-2-2 .9-2 2 .9 2 2 2zm7 0c1.1 0 2-.9 2-2s-.9-2-2-2-2 .9-2 2 .9 2 2 2z"
										></path>
									</g>
								</svg>
							</div>
						</div>
					</div>
					<div class="feed-item-text-wrapper">
						<div class="feed-item-text-body">
							<p class="feed-text">This is my tweet!!</p>
						</div>
					</div>
					<div class="feed-item-action-buttons">
						<div class="action-button-wrapper">
							<div class="action-button-image comments-button">
								<svg
									viewBox="0 0 24 24"
									aria-hidden="true"
									class="action-button-defaults comments-image"
								>
									<g>
										<path
											d="M 1.751 10 c 0 -4.42 3.584 -8 8.005 -8 h 4.366 c 4.49 0 8.129 3.64 8.129 8.13 c 0 2.96 -1.607 5.68 -4.196 7.11 l -8.054 4.46 v -3.69 h -0.067 c -4.49 0.1 -8.183 -3.51 -8.183 -8.01 Z m 8.005 -6 c -3.317 0 -6.005 2.69 -6.005 6 c 0 3.37 2.77 6.08 6.138 6.01 l 0.351 -0.01 h 1.761 v 2.3 l 5.087 -2.81 c 1.951 -1.08 3.163 -3.13 3.163 -5.36 c 0 -3.39 -2.744 -6.13 -6.129 -6.13 H 9.756 Z"
										></path>
									</g>
								</svg>
							</div>
							<div class="action-button-text comments-text">1253</div>
						</div>
						<div class="action-button-wrapper">
							<div class="action-button-image retweets-button">
								<svg viewBox="0 0 24 24" class="action-button-defaults retweets-image">
									<g>
										<path
											d="M4.5 3.88l4.432 4.14-1.364 1.46L5.5 7.55V16c0 1.1.896 2 2 2H13v2H7.5c-2.209 0-4-1.79-4-4V7.55L1.432 9.48.068 8.02 4.5 3.88zM16.5 6H11V4h5.5c2.209 0 4 1.79 4 4v8.45l2.068-1.93 1.364 1.46-4.432 4.14-4.432-4.14 1.364-1.46 2.068 1.93V8c0-1.1-.896-2-2-2z"
										></path>
									</g>
								</svg>
							</div>
							<div class="action-button-text retweets-text">1234</div>
						</div>
						<div class="action-button-wrapper">
							<div class="action-button-image likes-button">
								<svg viewBox="0 0 24 24" class="action-button-defaults likes-image">
									<g>
										<path
											d="M16.697 5.5c-1.222-.06-2.679.51-3.89 2.16l-.805 1.09-.806-1.09C9.984 6.01 8.526 5.44 7.304 5.5c-1.243.07-2.349.78-2.91 1.91-.552 1.12-.633 2.78.479 4.82 1.074 1.97 3.257 4.27 7.129 6.61 3.87-2.34 6.052-4.64 7.126-6.61 1.111-2.04 1.03-3.7.477-4.82-.561-1.13-1.666-1.84-2.908-1.91zm4.187 7.69c-1.351 2.48-4.001 5.12-8.379 7.67l-.503.3-.504-.3c-4.379-2.55-7.029-5.19-8.382-7.67-1.36-2.5-1.41-4.86-.514-6.67.887-1.79 2.647-2.91 4.601-3.01 1.651-.09 3.368.56 4.798 2.01 1.429-1.45 3.146-2.1 4.796-2.01 1.954.1 3.714 1.22 4.601 3.01.896 1.81.846 4.17-.514 6.67z"
										></path>
									</g>
								</svg>
							</div>
							<div class="action-button-text likes-text">1234</div>
						</div>
						<div class="action-button-wrapper">
							<div class="action-button-image views-button">
								<svg viewBox="0 0 24 24" class="action-button-defaults views-image">
									<g>
										<path
											d="M8.75 21V3h2v18h-2zM18 21V8.5h2V21h-2zM4 21l.004-10h2L6 21H4zm9.248 0v-7h2v7h-2z"
										></path>
									</g>
								</svg>
							</div>
							<div class="action-button-text views-text">1234</div>
						</div>
						<div class="action-button-wrapper">
							<div class="action-button-image share-button">
								<svg viewBox="0 0 24 24" class="action-button-defaults share-image">
									<g>
										<path
											d="M12 2.59l5.7 5.7-1.41 1.42L13 6.41V16h-2V6.41l-3.3 3.3-1.41-1.42L12 2.59zM21 15l-.02 3.51c0 1.38-1.12 2.49-2.5 2.49H5.5C4.11 21 3 19.88 3 18.5V15h2v3.5c0 .28.22.5.5.5h12.98c.28 0 .5-.22.5-.5L19 15h2z"
										></path>
									</g>
								</svg>
							</div>
						</div>
					</div>
				</div>
			</div>
			<% } %>
		</section>
		<section class="navbar-mobile">
			<div class="mobile-nav-icon-wrapper">
				<div class="mobile-nav-hover-wrapper">
					<img src="Icons/mobile_home_icon.svg" alt="" class="mobile-navbar-icon" />
				</div>
			</div>
			<div class="mobile-nav-icon-wrapper">
				<div class="mobile-nav-hover-wrapper">
					<img src="Icons/mobile_search_icon.svg" alt="" class="mobile-navbar-icon" />
				</div>
			</div>
			<div class="mobile-nav-icon-wrapper">
				<div class="mobile-nav-hover-wrapper">
					<img src="Icons/mobile_notification_icon.svg" alt="" class="mobile-navbar-icon" />
				</div>
			</div>
			<div class="mobile-nav-icon-wrapper">
				<div class="mobile-nav-hover-wrapper">
					<img src="Icons/mobile_messages_icon.svg" alt="" class="mobile-navbar-icon" />
				</div>
			</div>
		</section>
	</section>
</main>
